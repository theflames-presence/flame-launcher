{
    "files.eol": "\n",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": false
    },
    "typescript.preferences.quoteStyle": "single",
    "editor.tabSize": 2,
    "i18n-ally.enabledFrameworks": ["vue-sfc", "vue"],
    "i18n-ally.editor.preferEditor": true,
    "i18n-ally.keystyle": "nested",
    "i18n-ally.sortKeys": true,
    "typescript.tsdk": "xmcl-runtime/node_modules/typescript/lib",
    // "i18n-ally.keepFulfilled": true,
    "cSpell.words": [
        "bmcl",
        "Curseforge",
        "esbuild",
        "filenamify",
        "liteloader",
        "logined",
        "logining",
        "mcbbs",
        "Minecraft",
        "Modpack",
        "modpacks",
        "Modrinth",
        "mojang",
        "optifine",
        "resourcepack",
        "vite",
        "windicss",
        "xmcl"
    ],
    "cSpell.enabled": true,
    "yaml.schemas": {
        "https://json.schemastore.org/github-workflow.json": "file:///.github/workflows/deploy.yml"
    },
    "i18n-ally.localesPaths": [
        "xmcl-keystone-ui/locales",
    ],
    "npm.packageManager": "pnpm",
    "discord.enabled": true,
    "search.followSymlinks": false,
    "i18n-ally.keysInUse": [
        "curseforgeCategory.Armor, Tools, and Weapons",
        "curseforgeCategory.Utility & QoL",
        "curseforgeCategory.Vanilla+",
        "installForge.downloadInstaller",
        "dependencies.embedded"
    ],
    "oxc.enable": true,
    "oxc.configPath": "xmcl/packages/oxlint-config/oxlintrc.json"
}